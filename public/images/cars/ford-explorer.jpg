<svg width="300" height="200" viewBox="0 0 300 200" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="300" height="200" fill="#f8f9fa"/>
  
  <!-- SUV body (taller and more robust) -->
  <path d="M45 115 L65 95 L85 90 L215 90 L235 95 L255 115 L255 145 L45 145 Z" fill="#059669" stroke="#047857" stroke-width="2"/>
  
  <!-- SUV roof (higher) -->
  <path d="M75 95 L85 75 L215 75 L225 95" fill="#047857" stroke="#065f46" stroke-width="1"/>
  
  <!-- Windows -->
  <rect x="90" y="78" width="50" height="17" fill="#e0f2fe" stroke="#0369a1" stroke-width="1"/>
  <rect x="150" y="78" width="50" height="17" fill="#e0f2fe" stroke="#0369a1" stroke-width="1"/>
  
  <!-- Larger wheels for SUV -->
  <circle cx="80" cy="145" r="18" fill="#374151" stroke="#111827" stroke-width="2"/>
  <circle cx="80" cy="145" r="10" fill="#6b7280"/>
  <circle cx="220" cy="145" r="18" fill="#374151" stroke="#111827" stroke-width="2"/>
  <circle cx="220" cy="145" r="10" fill="#6b7280"/>
  
  <!-- Headlights -->
  <ellipse cx="250" cy="105" rx="8" ry="6" fill="#fef3c7"/>
  
  <!-- Grille -->
  <rect x="240" y="100" width="15" height="15" fill="#374151"/>
  
  <!-- Roof rails -->
  <rect x="85" y="72" width="130" height="2" fill="#6b7280"/>
  
  <!-- Car model text -->
  <text x="150" y="175" text-anchor="middle" fill="#374151" font-family="Arial, sans-serif" font-size="14" font-weight="bold">Ford Explorer</text>
  <text x="150" y="190" text-anchor="middle" fill="#6b7280" font-family="Arial, sans-serif" font-size="12">SUV</text>
</svg>